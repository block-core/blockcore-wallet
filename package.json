    "@blockcore/dns": "0.0.13",
    "@blockcore/identity": "0.0.12",
    "@blockcore/web5-injector": "0.0.3",
    "@fontsource/material-icons": "^4.5.4",
    "@ipld/dag-cbor": "^8.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
